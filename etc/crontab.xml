<?xml version="1.0"?>
<!--
/**
 * Girit Safecharge crontab config.
 *
 * @category Girit
 * @package  Girit_Safecharge
 */
-->
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="safecharge_delete_old_request_log"
             instance="Girit\Safecharge\Cron\DeleteOldRequestLog"
             method="execute">
            <schedule>* * * * *</schedule>
        </job>
    </group>
</config>